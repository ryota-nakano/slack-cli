{
  "name": "@ryota-nakano/slack-cli",
  "version": "2.3.0",
  "description": "Terminal-based Slack chat client with thread support and mention autocomplete",
  "main": "src/index.js",
  "bin": {
    "slack-cli": "./src/index.js",
    "slack": "./src/index.js"
  },
  "scripts": {
    "start": "node src/index.js channel",
    "channels": "node src/index.js channels",
    "setup": "node src/index.js setup",
    "config": "node src/index.js config",
    "help": "node src/index.js --help"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ryota-nakano/slack-cli.git"
  },
  "publishConfig": {
