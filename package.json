  },
  "keywords": [
    "slack",
    "cli",
    "chat",
    "terminal",
    "thread",
    "mention"
  ],
  "author": "ryota-nakano",
  "license": "ISC",
  "type": "commonjs",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@slack/web-api": "^7.12.0",
    "chalk": "^4.1.2",
    "dotenv": "^17.2.3",
    "node-emoji": "^2.2.0",
    "string-width": "^4.2.3"
  }
}
